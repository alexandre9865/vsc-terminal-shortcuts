{
  "name": "run",
  "displayName": "run",
  "description": "run the current file",
  "version": "0.0.7",
  "publisher": "local-run",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lglong519/vscode-extension.git"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.run",
        "title": "run"
      }
    ],
    "keybindings": [
      {
        "command": "extension.run",
        "key": "shift+alt+s"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "eslint-config-teslint": "latest",
    "vscode": "^1.1.6"
  }
}
