{
  "name": "run",
  "displayName": "run",
  "description": "run the current file",
  "version": "0.0.1",
  "publisher": "local-run",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lglong519/vscode-extension.git"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onCommand:extension.run",
    "onCommand:extension.clear"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.run",
        "title": "run"
      }
    ],
    "keybindings": [
      {
        "command": "extension.run",
        "key": "shift+alt+s"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "eslint": "^4.11.0",
    "eslint-config-lglong519": "github:lglong519/eslint-config",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}
